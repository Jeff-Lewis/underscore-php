{
  "name": "jeff-lewis/underscore-php",
  "description": "A redacted port of Underscore.js for PHP",
  "license": "MIT",
  "keywords": [
    "toolkit",
    "internals",
    "laravel"
  ],
  "authors": [
    {
      "name": "Jeff Lewis",
      "email": "jlewis@streetconnect.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "doctrine/inflector": "^1.3.0",
    "patchwork/utf8": "^1.3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.11.1",
    "phar-io/manifest": "dev-master",
    "phpunit/phpunit": "^7.1.5"
  },
  "autoload": {
    "psr-4": {
      "Underscore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Underscore\\": "tests"
    }
  },
  "scripts": {
    "test": ["php-cs-fixer fix --dry-run", "phpunit --verbose"],
    "lint": "php-cs-fixer fix"
  }
}
